    "chai": "3.x.x",
    "enzyme": "^2.9.1",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "1.7.x",
    "eslint-plugin-babel": "2.x.x",
    "eslint-plugin-react": "3.x.x",
    "mocha": "2.x.x",
